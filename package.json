{
  "name": "cat",
  "displayName": "Simple Date Inserter",
  "description": "Вставляет текущую дату по cmd+shift+d",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.2"
  },
  "activationEvents": [
    "onCommand:extension.insertDate"
  ],
  "contributes": {
    "commands": [{
      "command": "extension.insertDate",
      "title": "Вставить дату"
    }],
    "keybindings": [{
      "command": "extension.insertDate",
      "key": "cmd+shift+d",
      "mac": "cmd+shift+d"
    }]
  }
}